{
  "name": "Visual Studio Code Dark Plus",
  "dark": true,
  "author": "Alexander Makarov",
  "editorScheme": "/visual_studio_code_dark_plus.xml",
  "ui": {
    "*": {
      "background": "#21252B"
     },

    "Borders": {
      "color": "#21252B",
      "ContrastBorderColor": "#21252B"
    },
    "EditorTabs": {
      "selectedBackground": "#1d1d1d",
      "underlinedTabBackground": "#1d1d1d",
      "selectedForeground": "#e6e6e6",
      "inactiveMaskColor": "#2c2c2c",
      "inactiveColoredFileBackground": "#2c2c2c",
      "underlineHeight": 0,
      "borderColor": "#21252B"
    },
    "Panel" : {
      "foreground": "#acacac",
      "background": "#21252B"
    },
    "Tree": {
      "background": "#222830",
      "foreground": "#c7c7c7",
      "selectionBackground": "#094770",
      "selectionForeground": "#f8fafb"
    },
    "Menu": {
      "background": "#21252B",
      "foreground": "#cbcbc2",
      "borderColor": "#3d3374",
      "separatorColor": "#3d3374"
    },
    "PopupMenu":{
      "background": "#21252B"
    },
    "PopupMenuSeparator": {
      "height": 15,
      "stripeIndent": 6,
      "stripeWidth": 1
    },
    "MenuBar": {
      "borderColor": "#21252B",
      "background": "#21252B"
    },
    "MenuItem": {
      "background": "#22c91c",
      "foreground": "#cbcbc2",
      "selectionBackground": "#094770"
    },
    "NavBar": {
      "background": "#21252B",
      "borderColor": "#21252B"
    },
    "CompletionPopup": {
      "background": "#21252B",
      "foreground": "#cecece"
    },
    "Button": {
      "arc": 2
    },
    "StatusBar": {
      "borderColor": "#21252B"
    },
    "ToolWindow": {
      "background": "#21252B",
      "Header": {
        "borderColor": "#21252B",
        "background": "#21252B",
        "inactiveBackground": "#21252B"
      }
    }
  }
}
